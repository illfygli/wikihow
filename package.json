    "cheerio": "^0.22.0",
    "envalid": "^4.1.2",
    "node-schedule": "^1.3.0",
    "pluralize": "^7.0.0",
    "source-map-support": "^0.5.8",
    "twoot": "0.0.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.8",
    "@types/node": "^10.5.8",
    "@types/node-schedule": "^1.2.2",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-typescript": "^0.12.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "ts-node-dev": "^1.0.0-pre.26",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "engines": {
    "node": ">=10.6.0"
  }
