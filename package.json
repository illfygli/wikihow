{
  "name": "wikihowbot",
  "version": "1.0.0",
  "description": "a bot that posts random wikihow images to mastodon.",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only --notify=false src/main.ts -- local",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:lint": "eslint ./src --ext .ts,.tsx",
    "test:prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' -l",
    "prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' --write"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.19.0",
    "canvas": "^2.5.0",
    "cheerio": "^0.22.0",
    "envalid": "^5.0.0",
    "masto": "^2.1.1",
    "source-map-support": "^0.5.12",
    "wordfilter": "^0.2.6"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.12",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=12.1.0"
  }
}
