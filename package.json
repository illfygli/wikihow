{
  "name": "wikihowbot",
  "version": "1.0.0",
  "description": "a bot that posts random wikihow images to mastodon.",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only --notify=false src/main.ts -- local",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:lint": "eslint 'src/**/*'",
    "test:prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' -l",
    "prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' --write"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@sentry/integrations": "^6.11.0",
    "@sentry/node": "^6.11.0",
    "axios": "^0.21.1",
    "canvas": "^2.8.0",
    "cheerio": "^0.22.0",
    "envalid": "^6.0.2",
    "masto": "^4.1.9",
    "source-map-support": "^0.5.19",
    "wordfilter": "^0.2.6"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.22",
    "@types/node": "^16.6.1",
    "eslint": "^7.32.0",
    "eslint-config-lostfictions": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "^16.6.2"
  }
}
