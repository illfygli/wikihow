{
  "name": "wikihowbot",
  "version": "1.0.0",
  "description": "a bot that posts random wikihow images to mastodon.",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only --notify=false src/main.ts -- local",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:lint": "eslint 'src/**/*'",
    "test:prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' -l",
    "prettier": "prettier 'src/**/*.{js,ts,jsx,tsx}' --write"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.20.0",
    "canvas": "^2.6.1",
    "cheerio": "^0.22.0",
    "envalid": "^6.0.2",
    "masto": "^3.5.1",
    "source-map-support": "^0.5.19",
    "wordfilter": "^0.2.6"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/node": "^14.11.2",
    "eslint": "^7.9.0",
    "eslint-config-lostfictions": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=12.1.0"
  },
  "resolutions": {
    "**/needle": "^2.5.2"
  }
}
